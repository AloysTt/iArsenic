{
    "extends": "semistandard",
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
      "comma-dangle": ["error", "always-multiline"],
      "space-before-function-paren": ["error", {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }]
    }
}
